{{#each this}}
<li class='events'>
    <div class="card">        
        <img class="card-foto" src="{{images.0.url}}" width="180" height="227" alt=""/>        
        <div class="info">
            <p class="team">{{name}}</p>
            <p class="start-date">{{dates.start.localDate}}</p>
            <p class="place">{{_embedded.venues.0.name}}</p>           
        </div>
    </div>
</li>
{{/each}}